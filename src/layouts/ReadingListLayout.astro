---
import Layout from "./Layout.astro";
const crumbs = [
	{
		name: "home",
		link: "/",
		disabled: false,
	},
	{
		name: "Reading Lists",
		link: "/reading-lists",
		disabled: true,
	},
];

const frontmatter = Astro.props

---

<Layout title={frontmatter.title} crumbs={crumbs}>
        <slot />
</Layout>
